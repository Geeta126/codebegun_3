import java.util.*;


public class Array_List  {
	public static void main(String[] args) {
		List list= new ArrayList();
		
        list.add(87);
		list.add("geeta");
		list.add(567);
		
		System.out.println("Adding elements ::"+list);
		list.remove(0);
		System.out.println("Remove elements ::"+list);
		list.set(0, 666);
		System.out.println("Update elements ::"+list);
		
		
	}

}
