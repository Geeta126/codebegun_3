
public class Example {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int i,count=0;
		int[] x= {1,2,3,4,5};
		for(i=0;i<x.length;i++) {
			if(x[i]%2==0) {
				count=count+1;
			}else if((x[i]%2 !=0)&& (x[i]==5)) {
				count=count+3;
			}
			
		}
			
		}


	}
		

	}


